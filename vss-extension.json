{
    "manifestVersion": 1,
    "id": "coderr.azuredevops.tasks",
    "name": "Build and Release tasks for Coderr Server",
    "version": "0.0.1",
    "publisher": "1TCompany AB",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Tasks to clear and check errors for specific environments in Coderr Server.",
    "categories": [
        "Azure Pipelines",
        "Error Handling",
        "Exception Handling"
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ]
}